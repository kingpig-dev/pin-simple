{
  "name": "pin-simple",
  "version": "0.0.0",
  "description": "Pinterest, but just the pictures",
  "author": "Brittany Chiang <brittany.chiang@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bchiang7/pin-simple/issues"
