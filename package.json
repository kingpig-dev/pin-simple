    "request": "^2.88.0"
  },
  "devDependencies": {
    "@upstatement/prettier-config": "^0.3.0",
    "concurrently": "^4.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  }
}
